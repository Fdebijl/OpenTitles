{
  "name": "opentitles.client",
  "private": true,
  "version": "2.3.0",
  "scripts": {
    "test": "jasmine-ts --config=jasmine.config.json",
    "build": "parcel build src/* --no-minify",
    "package": "crx3 -z OpenTitles.zip -o OpenTitles.crx dist",
    "lint": "eslint \"./src/**/*.ts\" --max-warnings 2",
    "publish-chrome": "webstore upload --source OpenTitles.zip --auto-publish --extension-id ipcpballelfolmocdhfjijgmbljachog --client-id $CWS_CLIENT_ID --client-secret $CWS_CLIENT_SECRET --refresh-token $CWS_REFRESH_TOKEN"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chrome": "0.0.121",
    "@types/firefox-webext-browser": "78.0.0",
    "@types/jasmine": "3.5.11",
    "@types/jsdom": "16.2.3",
    "@types/node": "14.0.24",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "chrome-webstore-upload-cli": "1.2.0",
    "crx3": "1.1.2",
    "eslint": "7.5.0",
    "jasmine": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "jasmine-ts": "0.3.0",
    "jsdom": "16.3.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-static-files-copy": "2.4.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "staticFiles": {
    "staticPath": "src/static",
    "watcherGlob": false
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
